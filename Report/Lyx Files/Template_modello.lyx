\begin_layout Plain Layout

tof_1 = (x(2)-x(1))*86400;
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

% Compute the Lambert arc for tof_1 
\end_layout

\begin_layout Plain Layout

[~,~,~,~,VI_mars,VF_saturn,~,~] = lambertMR(r_mars,r_saturn,tof_1,ksun);
 
\end_layout

\begin_layout Plain Layout

dv1_mars = norm(VI_mars - v_mars'); 
