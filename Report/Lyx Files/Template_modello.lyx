\begin_layout Plain Layout

dv2_saturn = norm(v_saturn' - VF_saturn); 
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

% TOF to go from Saturn to Neptune, in days 
\end_layout

\begin_layout Plain Layout

tof_2 = (x(3)-x(2))*86400;
\end_layout

